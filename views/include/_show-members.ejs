<% for(var i = 0; i < users.length; i++) { %>
    <tr>
        <td><%= i + 1 %></td>
        <td><%= users[i].name %></td>
        <td><%= users[i].gouvernement %></td>
        <td><%= users[i].city %></td>
        <td><%= users[i].ministryname %></td>
        <td><%= users[i].workname %></td>
        <td><%= users[i].points %></td>
    </tr>
<% } %>